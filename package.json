{
  "name": "with-styled-components",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.js\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration:open": "cypress open",
    "test:integration": "cypress run --headless",
    "test:integration:build": "start-server-and-test 'yarn build/start' http-get://localhost:3000 'yarn test:integration'",
    "build/start": "yarn build && yarn start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.0.1",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^26.0.22",
    "eslint-plugin-jest": "^24.3.2",
    "framer-motion": "^3.7.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "next": "latest",
    "nookies": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-is": "^18.0.0",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.7",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "cypress": "^6.8.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.1"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
